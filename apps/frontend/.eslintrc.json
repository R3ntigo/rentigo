{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"../../.eslintrc.ts.json",
		"../../.eslintrc.react.json"
	],
	"parserOptions": {
		"project": "apps/frontend/tsconfig.eslint.json"
	},
	"plugins": [
		"jsx-a11y"
	],
	"ignorePatterns": [
		"!**/*"
	],
	"overrides": [
		{
			"files": [
				"*.ts",
				"*.tsx",
				"*.js",
				"*.jsx"
			],
			"rules": {}
		},
		{
			"files": [
				"*.ts",
				"*.tsx"
			],
			"rules": {}
		},
		{
			"files": [
				"*.js",
				"*.jsx"
			],
			"rules": {}
		}
	]
}
